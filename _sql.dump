#!/bin/bash

: "${DIR_RES:=_data}"
mkdir -p "${DIR_RES}"

set -x

### Init table
# Access without password assumed! Password must be stored in ~/.pgpass:
# echo 127.0.0.1:5432:filedupes:filedupes_u:<password> >> ~/.pgpass
podman exec hashingdb_db_1 pg_dump --compress=9 -U filedupes_u -h 127.0.0.1 --dbname filedupes > "${DIR_RES}/filedupes.dump.sql.gz"
